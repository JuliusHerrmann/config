{
  "pyprland": {
    "plugins": [
      "scratchpads",
      "workspaces_follow_focus"
    ]
  },
  "scratchpads": {
    "term": {
      "command": "alacritty --class alacritty-scratch",
      "animation": "",
      "lazy": "false",
      "unfocus": "false"
    },
    "calc": {
      "command": "alacritty --class alacritty-kalker -e kalker",
      "animation": "",
      "lazy": "true",
      "unfocus": "false"
    },
    "volume": {
      "command": "pavucontrol --name=pavu-scratch",
      "animation": "",
      "unfocus": "false",
      "lazy": "false"
    }
  }
}
